{
  "name": "function-visualizer-vscode",
  "displayName": "Function Visualizer",
  "description": "Visualize function calls and code structure in an interactive graph",
  "version": "0.1.1",
  "publisher": "YashGedia",
  "icon": "extension/resources/icon.png",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Visualization",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "function",
    "visualizer",
    "call graph",
    "code analysis",
    "javascript",
    "typescript",
    "python",
    "java"
  ],
  "activationEvents": [
    "onCommand:function-visualizer.debug",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:python",
    "onLanguage:java",
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:r",
    "onLanguage:go",
    "onLanguage:rust",
    "onLanguage:ruby",
    "onLanguage:php",
    "onLanguage:swift"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "function-visualizer.debug",
        "title": "Visualize: Debug Activation"
      },
      {
        "command": "function-visualizer.visualizeFile",
        "title": "Visualize: Show Function Graph",
        "icon": "$(graph)"
      },
      {
        "command": "function-visualizer.visualizeSelection",
        "title": "Visualize: Show Selection Graph"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "function-visualizer.debug",
          "group": "navigation"
        },
        {
          "command": "function-visualizer.visualizeFile",
          "group": "navigation",
          "when": "editorLangId =~ /^(javascript|typescript|python|java|javascriptreact|typescriptreact|c|cpp|r|go|rust|ruby|php|swift)$/"
        },
        {
          "command": "function-visualizer.visualizeSelection",
          "group": "navigation",
          "when": "editorHasSelection && editorLangId =~ /^(javascript|typescript|python|java|javascriptreact|typescriptreact|c|cpp|r|go|rust|ruby|php|swift)$/"
        }
      ],
      "commandPalette": [
        {
          "command": "function-visualizer.visualizeFile",
          "when": "editorLangId =~ /^(javascript|typescript|python|java|javascriptreact|typescriptreact|c|cpp|r|go|rust|ruby|php|swift)$/"
        },
        {
          "command": "function-visualizer.visualizeSelection",
          "when": "editorHasSelection && editorLangId =~ /^(javascript|typescript|python|java|javascriptreact|typescriptreact|c|cpp|r|go|rust|ruby|php|swift)$/"
        }
      ]
    },
    "keybindings": [
      {
        "command": "function-visualizer.visualizeFile",
        "key": "ctrl+shift+v f",
        "mac": "cmd+shift+v f",
        "when": "editorTextFocus"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yash19815/function-visualizer"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "*",
    "cmdk": "^1.1.1",
    "dagre": "^0.8.5",
    "embla-carousel-react": "^8.6.0",
    "html-to-image": "^1.11.13",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "re-resizable": "^6.11.2",
    "react": "^18.3.1",
    "react-cytoscapejs": "^2.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-resizable-panels": "^2.1.7",
    "reactflow": "^11.11.0",
    "recharts": "^2.15.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "*",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.107.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vscode/vsce": "^3.7.1",
    "autoprefixer": "^10.4.24",
    "css-loader": "^7.1.3",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.8.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.19",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vite": "6.3.5",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "vscode:prepublish": "npm run build:extension",
    "compile": "webpack --config extension/webpack.config.js --mode production",
    "watch": "webpack --config extension/webpack.config.js --mode development --watch",
    "dev": "cd webapp && vite",
    "build:webapp": "cd webapp && vite build",
    "build:extension": "webpack --config extension/webpack.config.js --mode production",
    "build": "npm run build:webapp && npm run build:extension",
    "preview": "cd webapp && vite preview",
    "format": "prettier --write .",
    "package": "vsce package --out packages/"
  }
}
